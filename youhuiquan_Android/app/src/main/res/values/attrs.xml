<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AppBackBar">
        <attr name="bar_title" format="string" />
        <attr name="bar_start_src" format="reference" />
        <attr name="bar_end_text" format="string" />
        <attr name="bar_last_src" format="reference" />
        <attr name="bar_last_second_src" format="reference" />
    </declare-styleable>
    <declare-styleable name="LineTop">
        <attr name="title" format="string" />
        <attr name="colorStyle" format="enum">
            <enum name="black" value="0" />
            <enum name="white" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <attr name="circle" format="boolean" />
        <attr name="radius" format="dimension" />
    </declare-styleable>


    <declare-styleable name="TuiACustom">
        <attr name="is_show_close" format="boolean" />
        <attr name="ad_width" format="dimension" />
        <attr name="ad_height" format="dimension" />
        <attr name="ad_placeholder" format="reference" />
    </declare-styleable>

    <declare-styleable name="EmptyView">
        <attr name="emptyView_text" format="string" />
        <attr name="emptyView_textSize" format="dimension" />
        <attr name="emptyView_textColor" format="reference|color" />
        <attr name="emptyView_imgSrc" format="reference" />
        <attr name="emptyView_marginTop" format="dimension" />
    </declare-styleable>
    <declare-styleable name="SortLetterView">
        <attr name="letterSize" format="dimension" />
        <attr name="leftBigText" format="dimension" />
        <attr name="paddingRight" format="dimension" />
        <attr name="iconWidth" format="dimension" />
        <attr name="iconHeight" format="dimension" />
        <attr name="leftIconPadding" format="dimension" />
        <attr name="letterPadding" format="dimension" />
        <attr name="offSetX" format="dimension" />
        <attr name="backgroundIconId" format="reference" />
        <attr name="letterColor" format="color" />
        <attr name="selectLetterColor" format="color" />
        <attr name="selectbigtTextColor" format="color" />
        <attr name="selectBackgroundColor" format="color" />
        <attr name="isLetterCenter" format="boolean" />
    </declare-styleable>
    <declare-styleable name="OnlyCheckBox">
        <attr name="drawable_checked" format="reference" />
        <attr name="drawable_unchecked" format="reference" />
        <attr name="is_checked" format="boolean" />
    </declare-styleable>
    <declare-styleable name="RoundImageView2">
        <attr name="style" format="enum">
            <enum name="circle" value="1" />
            <enum name="round" value="2" />
        </attr>
        <attr name="cornerRadius" format="dimension" />
    </declare-styleable>
</resources>